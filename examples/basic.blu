// int is a copy type and isn't moved
fn foo(x, y: int) int {
	let z = 10;
	return x + y * z;
}

fn take_and_set(x: &int) void {
	x = 10;
}

struct Vec2 {
	x: int,
	y: int,
}

fn vec2_new(x, y: int) Vec2 {
	// Maybe allow same name identifiers to be passed in without label?
	return Vec2 { x: x, y: y };
}

let fooret = foo(x: 10, y: 20);
take_and_set(x: &fooret);